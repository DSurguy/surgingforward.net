---
import {
  h2 as Header2,
  ul as UnList,
  li as List,
  a as Anchor
} from '@/components/blogComponents'

interface Props {
  links: string[]
}

const { links } = Astro.props;
---

<Header2 className="pt-0">Links</Header2>
<UnList>
  {links.map(link => <List><Anchor href={link}>{link}</Anchor></List>)}
</UnList>